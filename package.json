{
  "name": "windups",
  "version": "1.1.7",
  "description": "A unique typewriter effect library for React",
  "author": "Sam Gwilym <gwilym@me.com>",
  "license": "MIT",
  "homepage": "https://windups.gwil.co",
  "private": false,
  "main": "dist/index.js",
  "module": "dist/windups.esm.js",
  "size-limit": [
    {
      "path": "dist/windups.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/windups.esm.js",
      "limit": "10 KB"
    }
  ],
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/windups.esm.js"
    }
  },
  "peerDependencies": {
    "react": ">16.8.0"
  },
  "repository": "github:sgwilym/windups",
  "dependencies": {
    "break-styled-lines": "^1.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.9.0",
    "@size-limit/preset-small-lib": "^4.9.1",
    "@testing-library/react-hooks": "^2.0.1",
    "@types/jest": "^24.0.18",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^4.3.7",
    "jest": "^24.9.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-test-renderer": "^16.10.2",
    "size-limit": "^4.9.1",
    "ts-jest": "^26.5.1",
    "tsdx": "^0.14.1",
    "tslib": "^2.1.0",
    "typescript": "^4.1.3"
  },
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "resolutions": {
    "**/@typescript-eslint/eslint-plugin": "^4.1.1",
    "**/@typescript-eslint/parser": "^4.1.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "keywords": [
    "windup",
    "typewriter effect",
    "typewriter",
    "react",
    "hooks"
  ]
}
